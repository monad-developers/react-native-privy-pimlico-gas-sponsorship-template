{
  "expo": {
    "name": "rn-wallet-app",
    "slug": "rn-wallet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/monad-icon.png",
    "scheme": "rnwalletapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.anonymous.rn-wallet-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/monad-icon.png",
        "resizeMode": "contain",
        "imageSize": 50,
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.rnwalletapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/monad-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "17.5"
          },
          "android": {
            "compileSdkVersion": 35
          }
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "677c3b3b-e9db-46e7-9d89-fac88aae2b40"
      }
    }
  }
}
